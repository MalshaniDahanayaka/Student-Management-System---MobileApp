        tools:layout="@layout/fragment_add_new_lecture_notes" >
        <action
            android:id="@+id/action_lecturer_add_new_lecture_notes_to_lecturer_Uploaded_lecture_notes"
            app:destination="@id/lecturer_Uploaded_lecture_notes" />
        <action
            android:id="@+id/action_lecturer_add_new_lecture_notes_to_connetion_error"
            app:destination="@id/connetion_error" />
    </fragment>


    <fragment
        android:id="@+id/lecturer_add_announcements_list"
        android:name="com.kelaniya.android.myapplication.ui.lecturer_announcements.LecturerAnnouncementsListFragment"
