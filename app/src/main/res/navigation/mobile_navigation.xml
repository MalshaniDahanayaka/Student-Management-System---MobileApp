            android:id="@+id/action_connetion_error_to_course_lecture_notes"
            app:destination="@id/course_lecture_notes" />
        <action
            android:id="@+id/action_connetion_error_to_nav_home3"
            app:destination="@id/nav_home" />
        <action
            android:id="@+id/action_connetion_error_to_nav_slideshow2"
            app:destination="@id/nav_slideshow" />
        <action
            android:id="@+id/action_connetion_error_to_signUp"
            app:destination="@id/signUp" />
        <action
            android:id="@+id/action_connetion_error_to_nav_profile"
            app:destination="@id/nav_profile" />
