<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/login">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.kelaniya.android.myapplication.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_nav_home_to_connetion_error"
            app:destination="@id/connetion_error" />
        <action
            android:id="@+id/action_nav_home_to_connetion_error2"
            app:destination="@id/connetion_error" />
        <action
            android:id="@+id/action_nav_home_to_about_course"
            app:destination="@id/about_course" />
        <action
            android:id="@+id/action_nav_home_to_course_lecture_notes"
            app:destination="@id/course_lecture_notes" />
        <action
            android:id="@+id/action_nav_home_to_connetion_error3"
            app:destination="@id/connetion_error" />
        <action
            android:id="@+id/action_nav_home_to_nav_profile"
            app:destination="@id/nav_profile" />
    </fragment>

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.kelaniya.android.myapplication.ui.my_courses.MyCoursesFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_my_courses" >
        <action
            android:id="@+id/action_nav_gallery_to_connetion_error"
            app:destination="@id/connetion_error" />
        <action
            android:id="@+id/action_nav_gallery_to_course_lecture_notes"
            app:destination="@id/course_lecture_notes" />
        <action
            android:id="@+id/action_nav_gallery_to_connetion_error2"
            app:destination="@id/connetion_error" />
    </fragment>

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.kelaniya.android.myapplication.ui.marks_and_grades.MarksAndGradesFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_marks_and_grades" >
        <action
            android:id="@+id/action_nav_slideshow_to_connetion_error"
            app:destination="@id/connetion_error" />
        <action
            android:id="@+id/action_nav_slideshow_to_connetion_error2"
            app:destination="@id/connetion_error" />
    </fragment>

    <fragment
        android:id="@+id/connetion_error"
        android:name="com.kelaniya.android.myapplication.api.ConnectionError"
        android:label="connection Error"
        tools:layout="@layout/fragment_connection_error" >
        <action
            android:id="@+id/action_connetion_error_to_nav_home"
