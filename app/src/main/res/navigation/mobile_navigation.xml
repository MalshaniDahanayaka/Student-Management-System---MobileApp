            android:id="@+id/action_course_announcement_full_body_to_course_announcement"
            app:destination="@id/course_announcement" />
    </fragment>


    <fragment
        android:id="@+id/nav_profile"
        android:name="com.kelaniya.android.myapplication.ui.profile.ProfileFragment"
        android:label="nav_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_nav_profile_to_connetion_error"
            app:destination="@id/connetion_error" />
        <action
            android:id="@+id/action_nav_profile_to_lecturer_home"
            app:destination="@id/lecturer_home" />
    </fragment>

    <fragment
        android:id="@+id/lecturer_home"
        android:name="com.kelaniya.android.myapplication.ui.lecturer_home.LecturerHomeFragment"
        android:label="lecturer_home"
        tools:layout="@layout/fragment_lecturer_home" >
        <action
            android:id="@+id/action_lecturer_home_to_connetion_error"
