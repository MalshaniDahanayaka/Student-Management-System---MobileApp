        <action
            android:id="@+id/action_lecturer_Uploaded_lecture_notes_to_connetion_error"
            app:destination="@id/connetion_error" />
        <action
            android:id="@+id/action_lecturer_Uploaded_lecture_notes_to_lecturer_add_new_lecture_notes"
            app:destination="@id/lecturer_add_new_lecture_notes" />
        <action
            android:id="@+id/action_lecturer_Uploaded_lecture_notes_to_lecturer_add_announcements_list"
            app:destination="@id/lecturer_add_announcements_list" />
        <action
            android:id="@+id/action_lecturer_Uploaded_lecture_notes_to_lecturer_enroll_students_list"
            app:destination="@id/lecturer_enroll_students_list" />
    </fragment>

    <fragment
        android:id="@+id/lecturer_add_new_course_module"
        android:name="com.kelaniya.android.myapplication.ui.add_new_course.AddNewCourseModuleFragment"
        android:label="lecturer add new  course"
        tools:layout="@layout/fragment_add_new_course_module" >
        <action
            android:id="@+id/action_lecturer_add_new_course_module_to_lecturer_home"
            app:destination="@id/lecturer_home" />
        <action
            android:id="@+id/action_lecturer_add_new_course_module_to_connetion_error"
            app:destination="@id/connetion_error" />
    </fragment>


    <fragment
        android:id="@+id/lecturer_add_new_lecture_notes"
        android:name="com.kelaniya.android.myapplication.ui.add_new_lecturer_notes.AddNewLectureNotesFragment"
