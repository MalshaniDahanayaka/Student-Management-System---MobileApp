            android:id="@+id/action_connetion_error_to_lecturer_add_announcements_list"
            app:destination="@id/lecturer_add_announcements_list" />
        <action
            android:id="@+id/action_connetion_error_to_lecturer_add_new_announcement"
            app:destination="@id/lecturer_add_new_announcement" />
