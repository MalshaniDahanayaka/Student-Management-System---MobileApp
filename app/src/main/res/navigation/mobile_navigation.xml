    <fragment
        android:id="@+id/login"
        android:name="com.kelaniya.android.myapplication.ui.login.LoginFragment"
        android:label="@string/login_fragment"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_login_to_nav_home"
            app:destination="@id/nav_home" />
        <action
            android:id="@+id/action_login_to_signUp"
            app:destination="@id/signUp" />
        <action
            android:id="@+id/action_login_to_connetion_error"
            app:destination="@id/connetion_error" />
        <action
            android:id="@+id/action_login_to_lecturer_home"
            app:destination="@id/lecturer_home" />
    </fragment>

    <fragment
        android:id="@+id/signUp"
        android:name="com.kelaniya.android.myapplication.ui.signUp.SingUpFragment"
        android:label="@string/action_sign_in"
        tools:layout="@layout/fragment_sing_up" >
        <action
            android:id="@+id/action_signUp_to_login"
            app:destination="@id/login" />
        <action
            android:id="@+id/action_signUp_to_connetion_error"
            app:destination="@id/connetion_error" />
        <action
            android:id="@+id/action_signUp_to_nav_home"
            app:destination="@id/nav_home" />
    </fragment>


    <fragment
        android:id="@+id/about_course"
        android:name="com.kelaniya.android.myapplication.ui.course_details.AboutCourseFragment"
        android:label="Course Details"
        tools:layout="@layout/fragment_about_course" >
        <action
            android:id="@+id/action_about_course_to_nav_home"
            app:destination="@id/nav_home" />
        <action
            android:id="@+id/action_about_course_to_course_lecture_notes"
            app:destination="@id/course_lecture_notes" />
        <action
            android:id="@+id/action_about_course_to_connetion_error"
            app:destination="@id/connetion_error" />
    </fragment>


    <fragment
        android:id="@+id/course_lecture_notes"
        android:name="com.kelaniya.android.myapplication.ui.course_lecture_notes.CourseLectureNotesFragment"
        android:label="Lecture Notes In Course"
        tools:layout="@layout/fragment_course_lecture_notes" >
        <action
            android:id="@+id/action_course_lecture_notes_to_connetion_error"
            app:destination="@id/connetion_error" />
        <action
            android:id="@+id/action_course_lecture_notes_to_course_announcement"
            app:destination="@id/course_announcement" />
    </fragment>

    <fragment
        android:id="@+id/course_announcement"
        android:name="com.kelaniya.android.myapplication.ui.notifications.NotificationListFragment"
        android:label="Notifications"
        tools:layout="@layout/fragment_notification_list" >
        <action
            android:id="@+id/action_course_announcement_to_course_lecture_notes"
            app:destination="@id/course_lecture_notes" />
        <action
            android:id="@+id/action_course_announcement_to_course_announcement_full_body"
            app:destination="@id/course_announcement_full_body" />
    </fragment>

    <fragment
        android:id="@+id/course_announcement_full_body"
        android:name="com.kelaniya.android.myapplication.ui.notifications.NotificationFullDetailsFragment"
