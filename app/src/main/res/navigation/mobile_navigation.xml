        <action
            android:id="@+id/action_lecturer_add_new_announcement_to_connetion_error"
            app:destination="@id/connetion_error" />
    </fragment>

    <fragment
        android:id="@+id/lecturer_enroll_students_list"
        android:name="com.kelaniya.android.myapplication.ui.lecturer_add_students_marks_and_grades.EnrollStudentsListFragment"
